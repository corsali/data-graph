{
  "cleanUrls": true,
  "functions": { "**/*/index.ts": { "memory": 1024 } },
  "routes": [{ "src": "api/rest/.*", "dest": "api/rest" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ]
}
